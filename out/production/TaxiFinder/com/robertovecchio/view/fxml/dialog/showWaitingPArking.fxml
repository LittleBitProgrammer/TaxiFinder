<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.robertovecchio.controller.dialog.ShowWaitingParkingController"
            headerText="Parcheggio in cui Ã¨ in sosta"
            prefHeight="400.0" prefWidth="600.0">

    <content>
        <GridPane alignment="CENTER" hgap="10" vgap="10">
            <Label text="Nome Parcheggio"
                   GridPane.rowIndex="0"
                   GridPane.columnIndex="0"/>

            <TextField fx:id="stationNameField"
                       editable="false"
                       GridPane.rowIndex="0"
                       GridPane.columnIndex="1"/>

            <Label text="Via"
                   GridPane.rowIndex="1"
                   GridPane.columnIndex="0"/>

            <TextField fx:id="streetNameField"
                       editable="false"
                       GridPane.rowIndex="1"
                       GridPane.columnIndex="1"/>

            <Label text="Civico"
                   GridPane.rowIndex="2"
                   GridPane.columnIndex="0"/>

            <TextField fx:id="streetNumberField"
                       editable="false"
                       GridPane.rowIndex="2"
                       GridPane.columnIndex="1"/>
        </GridPane>
    </content>

</DialogPane>
